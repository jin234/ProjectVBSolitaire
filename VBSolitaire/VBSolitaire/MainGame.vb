    Private Sub Show_deck_Click(sender As Object, e As EventArgs) Handles Show_deck.Click

        Dim deck As String = ""
        For Each i In Stage.Deck
            deck += i + " "
        Next

        Dim deck_open As String = ""
        For Each i In Stage.Deck_Open
            deck_open += i + " "
        Next

        Dim Row1 As String = ""
